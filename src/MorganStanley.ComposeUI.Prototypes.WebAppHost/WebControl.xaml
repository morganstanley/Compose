<UserControl x:Class="MorganStanley.ComposeUI.Prototypes.WebAppHost.WebControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MorganStanley.ComposeUI.Prototypes.WebAppHost"
             xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
             d:DataContext="{d:DesignInstance Type=local:WebApplication}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             DataContext="local:WebApplication">
    <Grid>
        <wv2:WebView2 Name="WebView" Source="{Binding CurrentUri}"></wv2:WebView2>
    </Grid>
</UserControl>
